[user]
	name = Mun Joon Teo
	email = teomunjoon101@gmail.com
[color]
	ui = auto 
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "status"]
	added = green bold
	changed = red bold
	deleted = red bold
	untracked = yellow bold
[alias]
	a = add
	br = branch
	c = commit
	com = commit -m
	comamend = commit --amend --no-edit
	amendmsg = commit --amend
	cnv = commit --no-verify
	comnv = commit --no-verify -m
	comamendnv = commit --amend --no-edit --no-verify
	amendmsgnv = commit --amend --no-verify
	cl = clone
	f = fetch
	chk = checkout
	newb = checkout -b
	lg = log --oneline
	pl = pull
	plom = pull origin master
	ps = push
	psf = push --force
	psuo = push --set-upstream origin
	r = rebase
	ri = rebase -i
	ri2 = rebase -i HEAD~2
	ri3 = rebase -i HEAD~3
	ri4 = rebase -i HEAD~4
	rc = rebase --continue
	ra = rebase --abort
	shd = show HEAD
	sh = stash
	sp = stash pop
	st = status
[core]
	editor = vim
[format]
	pretty = oneline
[diff]
	wsErrorHighlight = all
[pull]
	rebase = true

